<!-- start _comparison-table-rows.liquid -->
<style>
  {% if request.visual_preview_mode %}
    {% assign section_id_variable = 'id' %}
  {% else %}
    {% assign section_id_variable = section.id %}
  {% endif %}
  .table-element-scoped-{{ section_id_variable }} tbody td.other-brands-column {
     background-color: {{ block.settings.icon_cells_background_color }};
   }
   .table-element-scoped-{{ section_id_variable }}  tr td:nth-child(2){
     {% if block.settings.enable_product_column_border_top_gradient %}
       position: relative;
       border-top: none;
     {% else %}
       border-top: 1px solid {% if block.settings.product_column_border_top_color != blank and block.settings.product_column_border_top_color.alpha != 0 %}rgba({{ block.settings.product_column_border_top_color.red }}, {{ block.settings.product_column_border_top_color.green }}, {{ block.settings.product_column_border_top_color.blue }}, {{ block.settings.product_column_border_opacity | divided_by: 100.0 }}){% else %}rgba(0, 0, 0, 0){% endif %};
     {% endif %}
   }
   {% if block.settings.enable_product_column_border_top_gradient %}
     .table-element-scoped-{{ section_id_variable }} tbody  tr td:nth-child(2)::before {
       content: '';
       position: absolute;
       top: 0;
       left: 0;
       right: 0;
       height: 1px;
       background: {% if block.settings.product_column_border_top_gradient != blank %}{{ block.settings.product_column_border_top_gradient }}{% else %}linear-gradient(to right, #ff0000, #ffa500){% endif %};
       z-index: 1;
     }
   {% endif %}
   .table-element-scoped-{{ section_id_variable }} tbody td.feature-cell {
     background-color: {{ block.settings.features_background_color }};
     font-size: {{ block.settings.benefits_desktop_size }}px;
     text-align: left;
     padding-right: 2rem;
     padding-left: 2%;
     {% if block.settings.bold_features_text %}
       font-weight: bold;
     {% endif %}
   }
   @media (max-width: 1280px and min-width: 1024px) {
     .table-element-scoped-{{ section_id_variable }} tbody td.feature-cell {
       font-size: {{ block.settings.benefits_mobile_size }}px;
     }
   }
   .table-element-scoped-{{ section_id_variable }} .material-icon svg{
     width: {{ section.settings.icon_size }}px !important;
     height: {{ section.settings.icon_size }}px !important;
     fill: {{ section.settings.checkmark_color }} !important;
   }

   .table-element-scoped-{{ section_id_variable }} .other-brands-column .material-icon svg{
     fill: {{ section.settings.x_color }} !important;
   }
</style>

<tbody class="fade-in-child" {{ block.shopify_attributes }}>
  {% content_for 'blocks' %}
</tbody>
<!-- end _comparison-table-rows.liquid -->
