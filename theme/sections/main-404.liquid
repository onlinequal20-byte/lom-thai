        {
          "type": "header",
          "content": "t:sections.main-404.blocks.text.settings.styling.content"
        },
        {
          "type": "color",
          "id": "color",
          "label": "t:sections.all.common.body_color.label",
          "default": "#000000"
        },
        {
          "type": "color",
          "id": "highlighted_color",
          "label": "t:sections.all.common.bolded_text_color.label",
          "default": "#000000"
        },
        {
